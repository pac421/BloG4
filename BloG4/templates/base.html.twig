<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        <!-- FontAwesome -->
		<link rel="stylesheet" type="text/css" href="{{ asset('libraries/fontawesome/css/all.css') }}">
		<!-- MDBootstrap -->
		<link rel="stylesheet" type="text/css" href="{{ asset('libraries/mdbootstrap/css/bootstrap.min.css') }}">
		<link rel="stylesheet" type="text/css" href="{{ asset('libraries/mdbootstrap/css/mdb.min.css') }}">
		<link rel="stylesheet" type="text/css" href="{{ asset('libraries/mdbootstrap/css/style.css') }}">
		<!-- Custom -->
		<link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}">
		{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        <!-- jQuery -->
		<script type="text/javascript" src="{{ asset('libraries/jquery/jquery.min.js') }}"></script>
		<!-- TinyMCE -->
		<script type="text/javascript" src="{{ asset('libraries/tinymce/tinymce.min.js') }}"></script>		
		<!-- MDBootstrap -->
		<script type="text/javascript" src="{{ asset('libraries/mdbootstrap/js/jquery.min.js') }}"></script>
		<script type="text/javascript" src="{{ asset('libraries/mdbootstrap/js/popper.min.js') }}"></script>
		<script type="text/javascript" src="{{ asset('libraries/mdbootstrap/js/bootstrap.min.js') }}"></script>
		<script type="text/javascript" src="{{ asset('libraries/mdbootstrap/js/mdb.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
